{
  "name": "boilerplate-react-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean-up": "rm -rfd dist",
    "start:dev": "webpack-dev-server --host 0.0.0.0 --port 3000 --config=configs/webpack.dev.js",
    "start:prod": "yarn run clean-up && webpack -p --config=configs/webpack.prod.js && node express.js",
    "test": "jest --coverage --config=configs/jest.json",
    "codegen:generate": "apollo codegen:generate --addTypename --queries=./src/**/*.ts --schema=./schema.json --target=typescript --outputFlat=./src/schemaTypes.ts",
    "schema:download": "apollo schema:download --endpoint=http://localhost:8080/graphql",
    "refresh:types": "yarn schema:download && yarn codegen:generate"
  },
  "author": "Alexandru.Sabou",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^3.0.3",
    "@material-ui/icons": "^3.0.1",
    "@types/graphql": "^14.0.0",
    "@types/material-ui": "^0.21.5",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.1",
    "@types/superagent": "^3.8.4",
    "apollo-boost": "^0.1.16",
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-client": "^2.3.8",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "react": "^16.5.0",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.5.0",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.5.2",
    "superagent": "^4.0.0-beta.5",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.2",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "express": "^4.17.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.3",
    "react-hot-loader": "^4.3.6",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^4.4.1",
    "typescript": "^3.0.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-graphql-loader": "^1.0.0"
  }
}
